{
    "name": "shadowhand/cairon",
    "description": "Configuration wrapper for auryn dependency injector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Woody Gilk",
            "email": "woody.gilk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "rdlowrey/auryn": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cairon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cairon\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
